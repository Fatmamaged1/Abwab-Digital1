<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="<%= seo.description %>">
  <meta name="keywords" content="<%= seo.keywords %>">
  <meta name="title" content="<%= seo.title %>">
  <title><%= seo.title %></title>
  
  <!-- Structured Data for SEO -->
  <script type="application/ld+json">
    <%= JSON.stringify(seo.structuredData) %>
  </script>
</head>
<body>
  <header>
    <section class="hero">
        <img src="<%= homeData.heroSection.heroImage %>" alt="Hero Image">

      <h1><%= homeData.heroSection.title %></h1>
      <p><%= homeData.heroSection.description %></p>
    </section>
  </header>

  <main>
    <!-- About Section -->
    <section class="about">
      <img src="<%= homeData.aboutSection.image %>" alt="About Section Image">
      <p><%= homeData.aboutSection.description %></p>
    </section>

    <!-- Services Section -->
    <section class="services">
      <h2>Our Services</h2>
      <ul>
        <% homeData.services.forEach(service => { %>
          <li><%= service.name %>: <%= service.description %></li>
        <% }) %>
      </ul>
    </section>

    <!-- Testimonials Section -->
    <section class="testimonials">
      <h2>What Our Clients Say</h2>
      <ul>
        <% homeData.testimonials.forEach(testimonial => { %>
          <li>
            <p>"<%= testimonial.text %>"</p>
            <strong>- <%= testimonial.clientName %></strong>
          </li>
        <% }) %>
      </ul>
    </section>

    <!-- Trusted Partners Section -->
    <section class="trusted-partners">
      <h2>Our Trusted Partners</h2>
      <ul>
        <% homeData.trustedPartners.forEach(partner => { %>
          <li>
            <img src="<%= partner.logo %>" alt="<%= partner.name %>">
            <span><%= partner.name %></span>
          </li>
        <% }) %>
      </ul>
    </section>
  </main>

  <footer>
    <p>&copy; <%= new Date().getFullYear() %> Your Website Name</p>
  </footer>
</body>
</html>
